@model IEnumerable<ModuloPessoa.EnderecoEmail>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Pessoa.TipoPessoa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pessoa.TipoPessoa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {  id=item.rowguid }) |
                @Html.ActionLink("Details", "Details", new {  id=item.rowguid }) |
                @Html.ActionLink("Delete", "Delete", new {  id=item.rowguid })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
