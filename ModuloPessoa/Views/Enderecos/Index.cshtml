                @Html.DisplayFor(modelItem => item.LocalEspacial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.rowguid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataModificacao)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.EnderecoID }) |
                @Html.ActionLink("Details", "Details", new { id=item.EnderecoID }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.EnderecoID })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
