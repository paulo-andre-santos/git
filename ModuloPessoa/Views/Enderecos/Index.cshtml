            <th>
                @Html.DisplayNameFor(model => model.Cidade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodigoPostal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LocalEspacial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.rowguid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataModificacao)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
